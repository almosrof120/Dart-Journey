{
    "cSpell.enableFiletypes": [
        "dart"
    ],
    "cSpell.words": [
        "Mosrof",
        "Mymap"
    ],
    "cSpell.customDictionaries": {
        "custom-dictionary-workspace": {
            "name": "custom-dictionary-workspace",
            "path": "${workspaceFolder:dart basic}/.cspell/custom-dictionary-workspace.txt",
            "addWords": true,
            "scope": "workspace"
        }
    }
}